{
	"name": "@hqtsm/struct",
	"version": "0.0.0",
	"license": "MIT",
	"exports": {
		".": "./mod.ts",
		"./arr": "./arr.ts",
		"./bool": "./bool/mod.ts",
		"./bool/8": "./bool/8.ts",
		"./bool/16": "./bool/16.ts",
		"./bool/32": "./bool/32.ts",
		"./endian": "./endian.ts",
		"./float": "./float/mod.ts",
		"./float/16": "./float/16.ts",
		"./float/32": "./float/32.ts",
		"./float/64": "./float/64.ts",
		"./int": "./int/mod.ts",
		"./int/8": "./int/8.ts",
		"./int/16": "./int/16.ts",
		"./int/24": "./int/24.ts",
		"./int/32": "./int/32.ts",
		"./int/64": "./int/64.ts",
		"./member": "./member.ts",
		"./members": "./members.ts",
		"./native": "./native.ts",
		"./ptr": "./ptr.ts",
		"./struct": "./struct.ts",
		"./type": "./type.ts",
		"./util": "./util.ts"
	},
	"imports": {
		"@deno/dnt": "jsr:@deno/dnt@^0.41.3",
		"@hqtsm/dataview": "jsr:@hqtsm/dataview@^1.0.5",
		"@hqtsm/meek": "jsr:@hqtsm/meek@^1.0.1",
		"@std/assert": "jsr:@std/assert@^1.0.9"
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 80,
		"indentWidth": 4,
		"semiColons": true,
		"singleQuote": true,
		"proseWrap": "preserve",
		"exclude": [
			"**/*.yaml",
			"**/*.yml",
			".*/**/*.yaml",
			".*/**/*.yml"
		]
	},
	"lint": {
		"rules": {
			"exclude": [
				"no-boolean-literal-for-arguments"
			],
			"include": [
				"camelcase",
				"default-param-last",
				"eqeqeq",
				"explicit-function-return-type",
				"explicit-module-boundary-types",
				"guard-for-in",
				"no-await-in-loop",
				"no-boolean-literal-for-arguments",
				"no-console",
				"no-const-assign",
				"no-eval",
				"no-external-import",
				"no-implicit-declare-namespace-export",
				"no-non-null-asserted-optional-chain",
				"no-self-compare",
				"no-sparse-arrays",
				"no-sync-fn-in-async-fn",
				"no-throw-literal",
				"no-top-level-await",
				"no-undef",
				"prefer-ascii",
				"triple-slash-reference",
				"verbatim-module-syntax"
			]
		}
	},
	"exclude": [
		".*",
		"coverage",
		"docs",
		"vendor",
		"npm"
	],
	"publish": {
		"exclude": [
			"deno.lock",
			"**/*.test.ts",
			"scripts"
		]
	},
	"tasks": {
		"clean": "rm -rf coverage docs vendor npm",
		"test": "deno test --doc --parallel --shuffle --trace-leaks --coverage --clean",
		"docs": "deno doc --html mod.ts",
		"lint": "deno lint --fix",
		"linted": "deno lint",
		"format": "deno fmt",
		"formatted": "deno fmt --check",
		"publishable": "deno publish --allow-dirty --dry-run",
		"version": "deno run --allow-read --allow-write ./scripts/version.ts",
		"npm": "deno run --allow-env --allow-net --allow-read --allow-write --allow-run ./scripts/npm.ts"
	}
}
